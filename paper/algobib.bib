	file = {IEEE Xplore Abstract Record:/home/nherbaut/Documents/biblio/zotero/storage/KQP8FEDJ/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/nherbaut/Documents/biblio/zotero/storage/E7HZ4PSZ/Zhang et al. - 2015 - Unreeling Xunlei Kankan Understanding Hybrid CDN-.pdf:application/pdf}
